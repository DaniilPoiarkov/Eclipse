{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "ZiggyCreatures.Caching.Fusion": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "ZiggyCreatures.Caching.Fusion": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "Redis": "localhost:6379",
    "Default": "",
    "ApplicationInsights": "",
    "Emulator": "<use for cosmosdb emulator instance>"
  },
  "AllowedHosts": "*",

  "Telegram": {
    "Token": "<your-bot-token>",
    "Chat": "<chat-id>",
    "SecretToken": "<secret-token>",
    "ActiveEndpoint": "api/eclipse/_handle",
    "DisabledEndpoint": "api/eclipse/_disabled"
  },
  "Application": {
    "Chat": "<chat-id>"
  },
  "Authorization": {
    "JwtBearer": {
      "Issuer": "<issuer>",
      "Audience": "<audience>",
      "Key": "<secret key>"
    }
  },
  "App": {
    "SelfUrl": "https://localhost:7236"
  },
  "Google": {
    "Credentials": "<credentials>"
  },
  "Sheets": {
    "SheetId": "",
    "SuggestionsRange": ""
  },
  "Azure": {
    "CosmosOptions": {
      "DatabaseId": "",
      "Endpoint": "",
      "Container": ""
    }
  },
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com",
    "TenantId": "",
    "ClientId": "",
    "Scopes": [
      {
        "Name": "", 
        "Description": ""
      }
    ],
    "Urls": {
      "Authorization": "",
      "Token": "",
      "Refresh": ""
    }
  },
  "Localization": {
    "DefaultCulture": "en",
    "AvailableCultures": [
      "en",
      "uk"
    ]
  },
  "CultureList": [
    {
      "Culture": "English",
      "Code": "en"
    },
    {
      "Culture": "Ukrainian",
      "Code": "uk"
    }
  ],
  "ApiVersioningOptions": {
    "Header": "X-Api-Version",
    "DefaultVersion": 1.0,
    "ReportApiVersions": true,
    "AssumeDefaultVersionWhenUnspecified": true
  },
  "Settings": {
    "IsRedisEnabled": true,
    "IsGoogleEnabled": true,
    "IsDocker": false
  },
  "InboxMessages": {
    "ProcessCount": 20,
    "ConvertCount": 20
  }
}
