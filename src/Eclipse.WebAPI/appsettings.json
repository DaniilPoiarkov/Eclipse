{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Redis": "localhost:6379",
    "Default": "",
    "ApplicationInsights": "",
    "Emulator": "<use for cosmosdb emulator instance>"
  },
  "AllowedHosts": "*",

  "Telegram": {
    "Token": "<your-bot-token>",
    "Chat": "<chat-id>",
    "SecretToken": "<secret-token>",
    "ActiveEndpoint": "api/eclipse/_handle",
    "DisabledEndpoint": "api/eclipse/_disabled"
  },
  "Authorization": {
    "EclipseApiKey": "<web-api-access-token>",
    "JwtBearer": {
      "Issuer": "<issuer>",
      "Audience": "<audience>",
      "Key": "<secret key>"
    }
  },
  "App": {
    "SelfUrl": "https://localhost:7236"
  },
  "Google": {
    "Credentials": "<credentials>"
  },
  "Sheets": {
    "SheetId": "",
    "SuggestionsRange": ""
  },
  "Azure": {
    "CosmosOptions": {
      "DatabaseId": "eclipse",
      "Endpoint": ""
    }
  },
  "Localization": {
    "DefaultCulture": "en",
    "AvailableCultures": [
      "en",
      "uk"
    ]
  },
  "LanguageList": [
    {
      "Language": "English",
      "Code": "en"
    },
    {
      "Language": "Ukrainian",
      "Code": "uk"
    }
  ],
  "ApiVersioningOptions": {
    "Header": "X-Api-Version",
    "DefaultVersion": 1.0,
    "ReportApiVersions": true,
    "AssumeDefaultVersionWhenUnspecified": true
  },
  "Settings": {
    "IsRedisEnabled": true,
    "IsGoogleEnabled": true,
    "IsDocker": false
  }
}
