{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Redis": "localhost:6379",
    "Default": "",
    "ApplicationInsights": ""
  },
  "AllowedHosts": "*",

  "Telegram": {
    "Token": "<your-bot-token>",
    "Chat": "<chat-id>",
    "WebhookUrl": "<WebhookUrl>",
    "DisabledWebhookUrl": "<Disable-WebhookUrl>",
    "SecretToken": "<SecretToken>"
  },
  "Authorization": {
    "EclipseApiKey": "<web-api-access-token>"
  },
  "App": {
    "SelfUrl": "<app-url>"
  },
  "Google": {
    "Credentials": "<credentials>"
  },
  "Sheets": {
    "SheetId": "",
    "SuggestionsRange": ""
  },
  "Azure": {
    "CosmosOptions": {
      "DatabaseId": "",
      "ConnectionString": "",
      "Endpoint": ""
    }
  },
  "Localization": {
    "DefaultCulture": "en",
    "AvailableCultures": [
      "en",
      "uk"
    ]
  },
  "LanguageList": [
    {
      "Language": "English",
      "Code": "en"
    },
    {
      "Language": "Ukrainian",
      "Code": "uk"
    }
  ],
  "ApiVersioningOptions": {
    "Header": "X-Api-Version",
    "DefaultVersion": 1.0,
    "ReportApiVersions": true,
    "AssumeDefaultVersionWhenUnspecified": true
  },
  "IsRedisEnabled": true
}
